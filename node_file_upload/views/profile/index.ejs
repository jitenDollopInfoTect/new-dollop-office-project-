<div class="container mt-3">
    <div class="row">
        <div class="col-md-6 offset-md-3">
            <table class="table table-hover table-bordered table-dark table-stripped bg-dark table-striped">
                <tr>
                    <th>Full Name</th>
                    <td><%= result.name %></td>
                </tr>
                <tr>
                    <th>Email</th>
                    <td><%= result.username %></td>
                </tr>
                <tr>
                    <th>City</th>
                    <td><%= result.city %></td>
                </tr>
                <tr>
                    <th>Gender</th>
                    <td><%= result.gender %></td>
                </tr>
                <tr>
                    <th>Address</th>
                    <td><%= result.address %></td>
                </tr>
            </table>
        </div>
    </div>
</div>
